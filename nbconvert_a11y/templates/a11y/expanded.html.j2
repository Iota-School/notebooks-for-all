{% from "a11y/visibility.html.j2" import header_row, checkbox_row%}
<dialog id="nb-expanded-dialog">
    <form name="expanded">
        <button formmethod="dialog">Close</button>
        <table role="grid">
            <tbody>
                {% set labels = "cell source outputs" %}
                {{header_row(labels)}}
                {{checkbox_row("code", labels)}}
                {{checkbox_row("markdown", labels)}}
                {{checkbox_row("raw", labels)}}
            </tbody>
        </table>
    </form>
    {% include "a11y/activity-log.html.j2" %}

</dialog>